"\n"
"    \"[style[color:ffb045]]Dépêchez-vous.[[/style]]\""

#: src-en/data/events_sector_emv_gilded.xml:216
msgid ""
"data/events_sector_emv_gilded.xml$//textList[@name=\"EMV_GILDED_DELAY\"]/text[2]"
msgstr ""
