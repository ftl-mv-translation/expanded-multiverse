msgid ""
"data/events_sector_emv_droppoint.xml$//event[@name=\"EMV_MVPMC_FUEL_DISTRESS\"]/choice[4]/text"
msgstr "连连道歉，转身逃跑。"

#: src-en/data/events_sector_emv_droppoint.xml:79
msgid ""
"data/events_sector_emv_droppoint.xml$//event[@name=\"EMV_MVPMC_FUEL_DISTRESS\"]/choice[4]/event/text"
msgstr "你连声道歉后转身离开，留下那名被困的变元者一脸茫然地愣在原地。"

#: src-en/data/events_sector_emv_droppoint.xml:81
msgid ""
"data/events_sector_emv_droppoint.xml$//event[@name=\"EMV_MVPMC_FUEL_DISTRESS\"]/choice[4]/event/choice[1]/text"
msgstr "星舰管理。"

