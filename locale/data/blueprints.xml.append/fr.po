#: src-en/data/blueprints.xml.append:20
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"emv_mv_human_commander\"]/desc"
msgstr ""
"Une équipe humaine d'élite. Grâce à leur formation d'élite, ces gars peuvent "
"même saper le moral de leurs proies, les transformant ainsi en cibles "
"faciles."

#: src-en/data/blueprints.xml.append:22
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"emv_mv_human_commander\"]/title"
msgstr "Commandant Vaurien"

#: src-en/data/blueprints.xml.append:23
msgid ""
"data/blueprints.xml.append$//crewBlueprint[@name=\"emv_mv_human_commander\"]/short"
